    "import torch\n",
    "from torch.distributions import Normal\n",
    "\n",
    "MEAN = 0\n",
    "STD = 1\n",
    "\n",
    "gassian_kernel = Normal(MEAN, STD)\n",
    "exp_gaussian = torch.exp(gassian_kernel.sample(torch.Size([100000])))\n",
    "print(exp_gaussian.max(), exp_gaussian.min())\n"
   ]
  }
